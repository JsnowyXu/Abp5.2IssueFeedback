@using System.Threading
@using System.Web.Optimization
@using Abp.Configuration
@using Abp.Timing
@using Abp.Web.Mvc.Extensions
@using Fun2RepairMVC
@using Fun2RepairMVC.Configuration
@{
    Layout = null;
    SetAntiForgeryCookie();
}
<!DOCTYPE html>
<html lang="zh-CN">
<head>
    <!-- Required meta tags -->
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=11,IE=10,IE=9,IE=8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, user-scalable=0, minimum-scale=1.0, maximum-scale=1.0">
   @* @Html.Partial("_Title")*@
    @Styles.Render("~/Bundles/frontend/css")
    @Html.IncludeStyle("~/lib/font-awesome/css/font-awesome.min.css")
    @RenderSection("styles", required: false)
</head>
<body>
    <div class="wrapper">
        <!-- #Top Bar -->
        @Html.Partial("_Top")
        <!--Nav-->
        @Html.Partial("_Header")

        @RenderBody()

        @Html.Partial("_Footer")
        @Html.Partial("_FooterBar")
    </div>
    @Scripts.Render("~/Bundles/fontend/js/bottom")
    <!----//End-top-nav-script---->
    <script>
        //Localizing momentjs
        moment.locale('@Thread.CurrentThread.CurrentUICulture.Name');
    </script>

    @Html.IncludeScript("~/Abp/Framework/scripts/libs/abp.jquery.js")
    @*@Html.IncludeScript("~/Views/Shared/Layout.js")*@
    @Html.IncludeScript("~/js/main.js")
    @Html.IncludeScript("~/js/script.js")
    <!-- Dynamic scripts of ABP system (They are created on runtime and can not be bundled) -->
    <script src="~/api/AbpServiceProxies/GetAll?v=@(Clock.Now.Ticks)"></script>
    <script src="~/AbpScripts/GetScripts?v=@(Clock.Now.Ticks)" type="text/javascript"></script>
    <!-- SignalR scripts -->
    <script src="~/signalr/hubs"></script>
    @Html.IncludeScript("~/Abp/Framework/scripts/libs/abp.signalr.js")
    <!-- View specific scripts -->
    @RenderSection("scripts", required: false)
</body>
</html>
